<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
int[0,1] open = 0;
int[0,3] lock_state = 0;
int[0,9] x;
int lock[3] = {1,5,9};</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-187" y="-102">
		</location>
		<location id="id1" x="-187" y="-229">
		</location>
		<location id="id2" x="-323" y="-229">
		</location>
		<location id="id3" x="-442" y="-229">
			<name x="-467" y="-323">start</name>
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="-313" y="-365">x != lock[lock_state]</label>
			<label kind="assignment" x="-313" y="-331">lock_state := 0</label>
			<nail x="-187" y="-331"/>
			<nail x="-331" y="-331"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="guard" x="-314" y="-170">lock_state &lt; 3</label>
			<nail x="-314" y="-102"/>
			<nail x="-323" y="-102"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id3"/>
			<label kind="guard" x="-586" y="-178">lock_state == 3</label>
			<label kind="assignment" x="-544" y="-144">open:=1</label>
			<nail x="-442" y="-102"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="guard" x="-161" y="-195">x == lock[lock_state]</label>
			<label kind="assignment" x="-170" y="-161">lock_state ++</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="select" x="-289" y="-263">i:int[0,9]</label>
			<label kind="assignment" x="-272" y="-229">x:=i</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="guard" x="-425" y="-255">open == 0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
	</queries>
</nta>
